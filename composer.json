{
    "name": "ea-lab/passeplat",
    "description": "PassePlat is a tool which analyses web streams between initiator and destination.",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "brick/math": "^0.9",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": ">=1.7",
        "shrikeh/teapot": ">=2.3",
        "softcreatr/jsonpath": "^0.7 || ^0.8",
        "dakwamine/component-based-object": "^3.0",
        "dakwamine/component-based-object-event-dispatcher": "^2.0",
        "elasticsearch/elasticsearch": "~7.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Quang-Minh",
            "email": "quang-minh@emerya.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "PassePlat\\App\\": "src/",
            "PassePlat\\Core\\": "passeplat-core/src",
            "PassePlat\\Forms\\": "passeplat-forms/src",
            "PassePlat\\Tasks\\": "passeplat-tasks/src",
            "PassePlat\\Logger\\Elastic\\": "passeplat-logger-elastic/src"
        },
          "files": [
            "passeplat-tasks/init.emerya.passeplat-tasks.php",
            "passeplat-core/init.emerya.passeplat-core.php",
            "passeplat-logger-elastic/init.emerya.passeplat-logger-elastic.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
